
<%= form_for @song do |s|%>
  <%= s.label :title %>
  <%= s.text_field :title, @song.title %>
  <%= s.label :genre %>
  <%= s.text_field :genre, @song.genre %>
  <%= s.label :artist_name %>
  <%= s.text_field :artist_name, @song.artist_name %>
  <%= s.label :release_year %>
  <%= s.text_field :release_year, @song.release_year %>
  <%= s.label :released %>
  <%= s.check_box :released, @song.released %>
  <%end%>
